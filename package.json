{
  "name": "@cerebral/forms",
  "version": "0.0.0-next",
  "description": "A computed form",
  "main": "lib/index.js",
  "typings": "./index.d.ts",
  "scripts": {
    "test": "mocha --require babel-register \"src/**/*.test.js\"",
    "test:watch": "npm run test -- --watch",
    "build": "cross-env BABEL_ENV=production babel src/ --out-dir=lib/ -s",
    "coverage": "nyc --reporter=lcov --reporter=json npm run test",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cerebral/cerebral.git"
  },
  "keywords": ["forms"],
  "author": "Christian Alfoni <christianalfoni@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cerebral/cerebral/issues"
  },
  "homepage": "https://github.com/cerebral/cerebral/tree/master/packages/forms#readme",
  "dependencies": {},
  "nyc": {
    "exclude": ["node_modules", "lib", "tests", "**/*.test.js", "**/testHelper.js"]
  },
  "devDependencies": {
    "babel-plugin-transform-builtin-extend": "^1.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-cli": "^6.26.0",
    "babel-register": "^6.26.0",
    "cross-env": "^5.2.0",
    "mocha": "^5.2.0",
    "nyc": "^12.0.2"
  }
}
